<table>
    <thead>
        <tr>
            <th>Product Name</th>
            <th>Product Description</th>
            <th>Product Price</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
        <tr>
            <td><span>@item.ProductName</span></td>
            <td><span>@item.ProductDescription</span></td>
            <td><span>@item.ProductPrice</span></td>
        </tr>
        }
    </tbody>
</table>
